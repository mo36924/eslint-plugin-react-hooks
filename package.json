{
  "name": "@mo36924/eslint-plugin-react-hooks",
  "type": "module",
  "version": "0.0.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "ESLint rules for React Hooks",
  "author": "mo36924",
  "license": "MIT",
  "homepage": "https://github.com/mo36924/eslint-plugin-react-hooks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mo36924/eslint-plugin-react-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/mo36924/eslint-plugin-react-hooks/issues"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "react"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
